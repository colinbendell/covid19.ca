---
title: COVID-19 Vaccination Tracker
layout: layouts/home.njk
eleventyNavigation:
  key: Home
  order: 1
---

<style>
  {%- for province in canada %}
  #toggle-vaccine-{{ province.iso3166 }}:checked ~ table.vaccine > tbody > tr.healthregion.{{ province.iso3166 }} { display: table-row; }
  #toggle-vaccine-{{ province.iso3166 }}:checked ~ table.vaccine > tbody > tr.province.{{ province.iso3166 }} th:first-child .label-toggle::before { transform: rotate(90deg) rotateY(55deg);}
  #toggle-infection-{{ province.iso3166 }}:checked ~ table.infection > tbody > tr.healthregion.{{ province.iso3166 }} { display: table-row; }
  #toggle-infection-{{ province.iso3166 }}:checked ~ table.infection > tbody > tr.province.{{ province.iso3166 }} th:first-child .label-toggle::before { transform: rotate(90deg) rotateY(55deg);}
  {%- endfor %}
</style>
