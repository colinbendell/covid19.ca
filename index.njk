<h1><svg class="icon" aria-hidden="true" viewBox="0 0 512 512"><use xlink:href="#canada-mapleleaf"></use></svg>COVID-19 Status</h1>

{%- for province in canada %}
  <input id="toggle-vaccine-{{ province.iso3166 }}" class="toggle" type="checkbox">
  <input id="toggle-infection-{{ province.iso3166 }}" class="toggle" type="checkbox">
{%- endfor %}
<table class="vaccine table table-hover" width="100%" cellspacing="0">
  <thead>
  <tr>
    <th></th>
    <th colspan="6">Vaccinations</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>
  <tr>
    <th></th>
    <th>Vaccinations<br/>(60 days)</th>
    <th>Vaccinations<br/>Today</th>
    <th>Projected Completion</th>
    <th>Total</th>
    <th>1 Dose / Population</th>
    <th>Fully Vacc. / Population</th>
    <th>Infected / Population</th>
    <th>Vaccines Available</th>
  </tr>
  </thead>
  <tbody id="vaccinationsProvinceTable">
  {% macro vaccineRow(data, type='province') %}
    <tr class="vaccine {{ type }} {{ data.iso3166 or data.province }}"  title="Total: {{ data.today.total_vaccinations | d(0) | format }} ({{ data.today.date | readableDate }}: {{ data.today.change_vaccinations | format}}, {{ data.yesterday.date | readableDate }}: {{ data.yesterday.change_vaccinations | format }}) Population: {{ data.population | format }}">
      <th class="name" title="{{ data.data_status }} @ {{ data.updated_at }}">
      {% if type === 'province' %}
        <label for="toggle-vaccine-{{ data.iso3166 }}" class="label-toggle">
{#          <span class="balloon">#}
{#Status: <b>{{ data.data_status }}</b><br>#}
{#Last updated: <b>${updatedAt}</b><br>#}
{#Update expected by: <b>${expectedTime(data.iso3166)}</b>#}
{#          </span>#}
          <span class="name long">{{ data.name.en or data.name }}</span>
          <span class="name short">{{ data.iso3166 or data.name.en }}</span>
          <svg viewBox="0 0 512 512" class="province-update-status"><use xlink:href="#{{ data.data_status | lower | replace(r/\s/gi, '-') }}"></use></svg>
        </label>
      {% else %}
      <span class="name long">{{ data.name.en or data.name }}</span>
      <span class="name short">{{ data.short or data.name.en or data.name }}</span>
      {% endif %}
      </th>
      <td class="dose history">
        <div>
          <table class="charts-css column"><tbody>
            <tr>
              {%- for week in data.previousWeeks | head(-8) | head(7) %}
                {% set height = (week.change_vaccinations_avg / data.vaccine.maxVaccinations * 100) | round %}
                <td style="--size:calc({{ height if height > 0 else 3 }}/100);" ><span class="tooltip">Week of {{ week.start | readableDate }}: {{ week.change_vaccinations_avg | formatNumber }}</span></td>
              {%- endfor %}
            </tr>
            <tr>
              {%- for day in data.previous7Days %}
                {% set height = (day.change_vaccinations / data.vaccine.maxVaccinations * 100) | round %}
                <td style="--size:calc({{ height if height > 0 else 3}}/100);"><span class="tooltip">{{ day.date | readableDate }}: {{ day.change_vaccinations | formatNumber }}</span></td>
              {%- endfor %}
              {% set height = (data.today.change_vaccinations / data.vaccine.maxVaccinations * 100) | round %}
              <td style="--size:calc({{ height if height > 0 else 3 }}/100);"><span class="tooltip">{{ data.today.date | readableDate }}: {{ data.today.change_vaccinations | formatNumber }}</span></td>
            </tr>
            </tbody></table>
        </div>
      </td>
      <td class="dose today" data-rate="{{ data.vaccine.changeInVaccinationRate }}">{{ (data.today.change_vaccinations if data.today.change_vaccinations > 0 else data.lastWeekExclusive.change_vaccinations_avg) | formatNumber }}</td>
      <td class="supply complete {{ "bad" if data.vaccine.daysToFirstVaccinations > 15 else ( "ok" if data.vaccine.daysToFirstVaccinations > 5 else "good") }}">{{ data.vaccine.completeDate | readableDate if data.vaccine.daysToFirstVaccinations > 0 }}</td>
      <td class="dose total" >{{ data.today.total_vaccinations | d(0) | formatNumber }}</td>
      <td class="dose one">
        <div class="progressbar" role="progressbar" aria-valuenow="{{ data.today.vaccinationsPerCapita or "N/A" }}" data-value="{{ data.today.total_first_vaccination | max(0) | formatNumber }}" aria-labelledby="provinceID tableHeaderID"><span class="value" aria-hidden="true" style="width: {{ data.today.vaccinationsPerCapita}}%"></span>
          <span class="change percent" data-value="{{ data.today.change_vaccinationsPerCapita | format }}"></span>
          <span class="change absolute" data-value="{{ data.today.change_first_vaccination | format }}"></span>
        </div>
      </td>
      <td class="dose full">
        <div class="progressbar" role="progressbar" aria-valuenow="{{ data.today.vaccinationsCompletePerCapita or "N/A" }}" data-value="{{ data.total.total_vaccinated | formatNumber }}" aria-labelledby="provinceID tableHeaderID"><span class="value" aria-hidden="true" style="width: {{ data.today.vaccinationsCompletePerCapita or 0 }}%"></span>
          <span class="change percent" data-value="{{ data.today.change_vaccinationsCompletePerCapita | format }}"></span>
          <span class="change absolute" data-value="{{ data.total.change_vaccinated | format }}"></span>
        </div>
      </td>
      <td class="infected percapita">
        <div class="progressbar infection" role="progressbar" aria-valuenow="{{ data.today.casesPerCapita }}" data-value="{{ data.total.total_cases | formatNumber }}" aria-labelledby="provinceID tableHeaderID"><span class="value" aria-hidden="true" style="width: {{ (data.today.casesPerCapita/10*100) | min (100) }}%"></span></div>
      </td>
      <td class='supply available'>{{ data.today.available_doses | formatNumber }}
        <span class="supply zero {{ "bad" if data.vaccine.daysToZeroVaccines > 10 else ( "ok" if data.vaccine.daysToZeroVaccines >= 7 else "good") }}">{{ data.vaccine.daysToZeroVaccines }}</span>
      </td>
    </tr>
  {%  endmacro %}
  {%- for province in canada | filterProp("name", "neq", "Canada") | sort(true, attr="population") %}
  {{ vaccineRow(province) }}
    {%- for region in province.regions %}
      {{ vaccineRow(region, "healthregion") }}
    {%- endfor %}
  {%- endfor %}
  </tbody>
  <tfoot id="vaccinationsProvinceTableFooter">
  {%- for province in canada | filterProp("name", "eq", "Canada")  %}
    {{ vaccineRow(province, "country") }}
  {%- endfor %}
  </tfoot>
</table>

<table class="infection table table-hover" width="100%" cellspacing="0">
  <thead>
  <tr>
    <th></th>
    <th colspan="6">Infections</th>
    <th></th>
    <th></th>
  </tr>
  <tr>
    <th></th>
    <th>New Cases<br/>(60 days)</th>
    <th>New Cases<br/>Today</th>
    <th>Active Cases<br/>(60 days)</th>
    <th>Active/₁₀₀ₖ</th>
    <th>Hospital/₁₀₀₀ₖ</th>
    <th>Deaths/₁₀₀ₖ</th>
    <th>Test-Positivity<br/>Rate (7 days)</th>
    <th class="infected">Infected / Population</th>
    <th class="infected">Death Rate <br/>(of Infected)</th>
  </tr>
  </thead>
  <tbody id="casesProvinceTable">
  {% macro selectColour(data) %}{{ "green" if data < 10 else ("yellow" if data < 25 else ("orange" if data < 40 else ("red" if data < 100 else "black"))) }}{% endmacro %}
  {% macro selectBase(data) %}{{ 10 if data < 10 else (25 if data < 25 else (40 if data < 40 else (100 if data < 100 else 150))) }}{% endmacro %}

  {% macro infectionRow(data, type='province') %}
    <tr class="infection {{ type }} {{ data.iso3166 if data.iso3166 else data.province }}" title="Total: {{ data.total.total_cases | format }} ({{ data.total.date | readableDate }}: {{ data.total.change_cases | format }}, {{ data.yesterday.date | readableDate }}: {{ data.yesterday.change_cases | format }}) Population: {{ data.population | format }}">
      <th class="name" title="{{ data.data_status }} @ {{ data.updated_at }}">
        {% if type === 'province' %}
        <label for="toggle-infection-{{ data.iso3166 }}" class="label-toggle">
          {#          <span class="balloon">#}
          {#Status: <b>{{ data.data_status }}</b><br>#}
          {#Last updated: <b>${updatedAt}</b><br>#}
          {#Update expected by: <b>${expectedTime(data.iso3166)}</b>#}
          {#          </span>#}
          <span class="name long">{{ data.name.en or data.name }}</span>
          <span class="name short">{{ data.iso3166 or data.name.en }}</span>
          <svg viewBox="0 0 512 512" class="province-update-status"><use xlink:href="#{{ data.data_status | lower | replace(r/\s/gi, '-') }}"></use></svg>
        </label>
        {% else %}
        <span class="name long">{{ data.name.en or data.name }}</span>
        <span class="name short">{{ data.short or data.name.en or data.name }}</span>
        {% endif %}
      </th>
      <td class="cases history">
        <div>
          <table class="charts-css column"><tbody>
            <tr>
              {%- for week in data.previousWeeks | head(-8) | head(7) %}
                {% set height = (week.change_cases_avg / data.infection.maxChangeCases * 100) | round %}
                <td style="--size:calc({{ height if height > 0 else 3 }}/100);" ><span class="tooltip">Week of {{ week.start | readableDate }}: {{ week.change_cases_avg | formatNumber }}</span></td>
              {%- endfor %}
            </tr>
            <tr>
              {%- for day in data.previous7Days %}
                {% set height = (day.change_cases / data.infection.maxChangeCases * 100) | round %}
                <td style="--size:calc({{ height if height > 0 else 3}}/100);"><span class="tooltip">{{ day.date | readableDate }}: {{ day.change_cases | formatNumber }}</span></td>
              {%- endfor %}
              {% set height = (data.total.change_cases / data.infection.maxChangeCases * 100) | round %}
              <td style="--size:calc({{ height if height > 0 else 3 }}/100);"><span class="tooltip">{{ (data.total.date or data.total.latest_date) | readableDate}}: {{ data.total.change_cases | formatNumber}}</span></td>
            </tr>
            </tbody></table>
        </div>
      </td>
      <td class="activecases today" data-rate="{{ data.infection.changeInCasesRate }}" a="{{ data.today.change_cases }}" b="{{ data.lastWeekExclusive.active_cases_avg }}">{{ (data.today.change_cases if data.today.change_cases > 0 else data.lastWeekExclusive.change_cases_avg) | formatNumber }}</td>
      <td class="activecases history">
        <div>
          <table class="charts-css column"><tbody>
            <tr>
              {%- for week in data.previousWeeks | head(-8) | head(7) %}
                {% set height = (week.active_cases_avg / data.infection.maxActiveCases * 100) | round %}
                <td style="--size:calc({{ height if height > 0 else 3 }}/100);" ><span class="tooltip">Week of {{ week.start | readableDate}}: {{ week.active_cases_avg | formatNumber }}</span></td>
              {%- endfor %}
            </tr>
            <tr>
              {%- for day in data.previous7Days %}
                {% set height = (day.active_cases / data.infection.maxActiveCases * 100) | round %}
                <td style="--size:calc({{ height if height > 0 else 3 }}/100);" ><span class="tooltip">{{ day.date | readableDate}}: {{ day.active_cases | formatNumber }}</span></td>
              {%- endfor %}
              {% set height = (data.today.active_cases / data.infection.maxActiveCases * 100) | round %}
              <td style="--size:calc({{ height if height > 0 else 3 }}/100);"><span class="tooltip">{{ data.today.date | readableDate }}: {{ data.today.active_cases | formatNumber}}</span></td>
            </tr>
            </tbody></table>
        </div>
      </td>
      <td class="activecases per100k">
        <div class="progressbar {{ selectColour(data.today.activePer100k)  }}" aria-valuenow="{{ data.today.activePer100k }}" data-value="{{ data.today.active_cases | formatNumber }}" data-change="{{ data.today.change_active_cases | format }}" aria-labelledby="provinceID tableHeaderID"><span class="value" aria-hidden="true" style="width: {{ (data.today.activePer100k/selectBase(data.today.activePer100k)*100) | min(100) }}%"></span>
          <span class="change percent" data-value="{{ data.today.change_activePer100k | format }}"></span>
          <span class="change absolute" data-value="{{ data.today.change_active_cases | format }}"></span>
        </div>
      </td>
      <td class="hospitalized per1000k">
        <div class="progressbar {{ selectColour(data.today.hospitalizedPer1000k) }}" aria-valuenow="{{ data.today.hospitalizedPer1000k or "N/A"}}" data-value="{{ data.today.total_hospitalizations | formatNumber }}" aria-labelledby="provinceID tableHeaderID"><span class="value" aria-hidden="true" style="width: {{ (data.today.hospitalizedPer1000k/selectBase(data.today.hospitalizedPer1000k)*100) | min(100) }}%"></span>
          <span class="change percent" data-value="{{ data.today.change_hospitalizedPer1000k | format }}"></span>
          <span class="change absolute" data-value="{{ data.today.change_hospitalizations | format }}"></span>
        </div>
      </td>
      <td class="death per100k">
        <div class="progressbar death" role="progressbar" aria-valuenow="{{ data.today.fatalitiesPer100k }}" data-value="{{ data.today.total_fatalities | formatNumber }}" data-change="{{ data.today.change_fatalities | format }}" aria-labelledby="provinceID tableHeaderID"><span class="value" aria-hidden="true" style="width: {{ data.today.fatalitiesPer100k | min(100) }}%"></span>
          <span class="change percent" data-value="{{ data.today.change_fatalitiesPer100k | format }}"></span>
          <span class="change absolute" data-value="{{ data.today.change_fatalities | format }}"></span>
        </div>
      </td>
      <td class="positivity">
        <div class="progressbar" role="progressbar" aria-valuenow="{{ data.lastWeekInclusive.weekPositivityRate or "N/A"}}" data-value="{{ data.today.change_tests | formatNumber }}" data-change="{{ data.today.change_tests | format }}" aria-labelledby="provinceID tableHeaderID"><span class="value" aria-hidden="true" style="width: {{ ((data.lastWeekInclusive.weekPositivityRate or 0)/10*100) | min(100) }}%"></span>
          <span class="change percent" data-value="{{ (data.lastWeekInclusive.weekPositivityRate - ((data.previousWeeks | head(-2) | head(1))[0].weekPositivityRate) | d(0)) | format }}"></span>
        </div>
      </td>
      <td class="infected percapita"><div class="progressbar infection" role="progressbar" aria-valuenow="{{ data.today.casesPerCapita }}" data-value="{{ data.today.total_cases | formatNumber }}" aria-labelledby="provinceID tableHeaderID"><span class="value" aria-hidden="true" style="width: {{ (data.today.casesPerCapita/10*100) | d(0) | min(100) }}%"></span></div></td>
      <td class="infected death"><div class="progressbar death" role="progressbar" aria-valuenow="{{ data.today.deathsPerCase }}" data-value="{{ data.today.total_fatalities | formatNumber }}" aria-labelledby="provinceID tableHeaderID"><span class="value" aria-hidden="true" style="width: {{ (data.today.deathsPerCase/4*100) | d(0) | min(100) }}%"></span></div></td>
    </tr>
  {%  endmacro %}

  {%- for province in canada | filterProp("name", "neq", "Canada") | sort(true, attr="population") %}
    {{ infectionRow(province) }}
    {%- for region in province.regions %}
      {{ infectionRow(region, "healthregion") }}
    {%- endfor %}
  {%- endfor %}
  </tbody>
  <tfoot id="casesProvinceTableFooter">
  {%- for province in canada | filterProp("name", "eq", "Canada")  %}
    {{ infectionRow(province, "country") }}
  {%- endfor %}
  </tfoot>
</table>
